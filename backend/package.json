{
  "dependencies": {
    "aws-sdk": "^2.386.0",
    "bcrypt": "^3.0.3",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "typeorm": "^0.2.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.16",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "predev": "yarn types",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/resolvers/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "copytypes": "cp ./src/types/graph.d.ts ../frontend/types",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cp ./src/*.graphql ./dist/src/",
    "start": "node ./dist/src/index.js",
    "lint:js": "yarn eslint .",
    "lint": "yarn tslint 'src/**/*.ts?(x)'"
  }
}
